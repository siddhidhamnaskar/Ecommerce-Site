generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(cuid()) @map("_id")
  email String @unique
  password String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
